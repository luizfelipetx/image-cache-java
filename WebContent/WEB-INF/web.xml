<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>timthumb</display-name>
	<servlet>
		<description>
		</description>
		<display-name>ImageServlet</display-name>
		<servlet-name>ImageServlet</servlet-name>
		<servlet-class>com.skina.porygon.core.ImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/timthumb.php</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description>
		</description>
		<display-name>ImageServletDev</display-name>
		<servlet-name>ImageServletDev</servlet-name>
		<servlet-class>com.skina.porygon.core.ImageServletDev</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UploadAWS</display-name>
		<servlet-name>UploadAWS</servlet-name>
		<servlet-class>com.skina.porygon.core.UploadAWS</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageServletDev</servlet-name>
		<url-pattern>/timthumb2.php</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UploadAWS</servlet-name>
		<url-pattern>/UploadAWS</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<description>
		</description>
		<display-name>ImageCacheServlet</display-name>
		<servlet-name>ImageCacheServlet</servlet-name>
		<servlet-class>com.skina.porygon.core.ImageCacheServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>LabelsServlet</display-name>
		<servlet-name>LabelsServlet</servlet-name>
		<servlet-class>com.skina.porygon.core.LabelsServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>OpenPDF</display-name>
		<servlet-name>OpenPDF</servlet-name>
		<servlet-class>com.skina.porygon.core.OpenPDF</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>CleanPLP</display-name>
		<servlet-name>CleanPLP</servlet-name>
		<servlet-class>com.skina.porygon.core.CleanPLP</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageCacheServlet</servlet-name>
		<url-pattern>/api</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LabelsServlet</servlet-name>
		<url-pattern>/labels/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OpenPDF</servlet-name>
		<url-pattern>/open</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CleanPLP</servlet-name>
		<url-pattern>/clean</url-pattern>
	</servlet-mapping>
	
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
